#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int num1, num2, product;
    cin >> num1 >> num2;
    product = num1 * num2;
    int digitCount = (product == 0) ? 1 : static_cast<int>(log10(abs(product)) + 1);
    cout << "Product - " << product << endl;
    cout << "Count of digits - " << digitCount << endl;
    return 0;
}
